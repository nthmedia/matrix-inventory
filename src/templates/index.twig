{% extends "_layouts/cp" %}

{% set title = "Matrix Inventory" %}

{% set content %}
    <h2>Matrix Block Types</h2>
    <ul style="margin-left: 20px; list-style: disc;">
        {% for key, matrixField in matrixFields %}
            <li>
                <a href="/admin/matrix-inventory/view/{{ key }}">{{ matrixField.label }}</a>
            </li>
        {% endfor %}
    </ul>
{% endset %}